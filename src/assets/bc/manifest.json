{
  "name": "brocrobes",
  "version": "0.3.0",
  "description": "Browser plugin for https://transcrob.es",
  "homepage_url": "https://transcrob.es",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background-bundle.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "img/tc16.png",
    "32": "img/tc32.png",
    "64": "img/tc64.png",
    "128": "img/tc128.png"
  },
  "web_accessible_resources": [
    "img/*.*",
    "*.css"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Transcrobe Me!"
  },
  "manifest_version": 2
}
